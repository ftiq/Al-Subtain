<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="action_dynamic_lab_results_report" model="ir.actions.report">
        <field name="name">تقرير نتائج فحص الطابوق</field>
        <field name="model">lab.sample</field>
        <field name="report_type">qweb-pdf</field>
        <field name="report_name">appointment_products.dynamic_lab_results_template</field>
        <field name="report_file">appointment_products.dynamic_lab_results_template</field>
        <field name="binding_model_id" ref="model_lab_sample"/>
        <field name="binding_type">report</field>
        <field name="domain">[("sample_subtype_id.sample_type_id.code", "=", "MASONRY")]</field>
        <field name="paperformat_id" ref="base.paperformat_euro"/>
        <field name="groups_id" eval="[(6,0,[ref('appointment_products.group_lab_manager'), ref('appointment_products.group_lab_technician')])]"/>
    </record>


    <record id="action_ignitability_standard_report" model="ir.actions.report">
        <field name="name">تقرير قابلية الاشتعال</field>
        <field name="model">lab.sample</field>
        <field name="report_type">qweb-pdf</field>
        <field name="report_name">appointment_products.ignitability_standard_report</field>
        <field name="report_file">appointment_products.ignitability_standard_report</field>
        <field name="binding_model_id" ref="model_lab_sample"/>
        <field name="binding_type">report</field>
        <field name="domain">['|', ('sample_subtype_id.sample_type_id.code', '=', 'IGNITABILITY'), ('product_id.product_tmpl_id.sample_type_id.code', '=', 'IGNITABILITY')]</field>
        <field name="paperformat_id" ref="lab_sample_comprehensive_report_paperformat"/>
        <field name="print_report_name">'تقرير_قابلية_الاشتعال_%s' % (object.name)</field>
        <field name="groups_id" eval="[(6,0,[ref('appointment_products.group_lab_manager'), ref('appointment_products.group_lab_technician')])]"/>
    </record>

