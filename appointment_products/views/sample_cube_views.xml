<?xml version="1.0" encoding="utf-8"?>
<odoo>
    
        <record id="view_sample_cube_tree" model="ir.ui.view">
            <field name="name">lab.sample.cube.tree</field>
            <field name="model">lab.sample.cube</field>
            <field name="arch" type="xml">
                <list string="مكعبات الخرسانة" create="false" delete="false">
                    <field name="name"/>
                    <field name="sample_id"/>
                    <field name="set_no"/>
                    <field name="cube_index"/>
                    <field name="cube_type"/>
                    <field name="age_days"/>
                    <field name="truck_id"/>
                </list>
            </field>
        </record>

        <record id="view_sample_cube_form" model="ir.ui.view">
            <field name="name">lab.sample.cube.form</field>
            <field name="model">lab.sample.cube</field>
            <field name="arch" type="xml">
                <form string="مكعب خرسانة" create="false" delete="false">
                    <sheet>
                        <group>
                            <field name="name" readonly="1"/>
                            <field name="sample_id" readonly="1"/>
                            <field name="set_no" readonly="1"/>
                            <field name="cube_index" readonly="1"/>
                            <field name="cube_type" readonly="1"/>
                            <field name="age_days"/>
                            <field name="truck_id"/>
                        </group>
                    </sheet>
                </form>
            </field>
        </record>
    
</odoo> 