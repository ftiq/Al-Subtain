<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="view_calendar_event_form_inherit_service" model="ir.ui.view">
        <field name="name">calendar.event.form.inherit.service</field>
        <field name="model">calendar.event</field>
        <field name="inherit_id" ref="calendar.view_calendar_event_form"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='alarm_ids']" position="after">
                <field name="x_service_product_id"/>
                <field name="x_attachment_filename"/>
                <field name="x_attachment" filename="x_attachment_filename"/>
            </xpath>
        </field>
    </record>
</odoo>
