<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        

        <record id="cron_auto_escalate_tasks" model="ir.cron">
            <field name="name">Auto Escalate Overdue Approvals</field>
            <field name="model_id" ref="model_approval_request"/>
            <field name="state">code</field>
            <field name="code">model._cron_auto_escalate_overdue_approvals()</field>
            <field name="interval_number">1</field>
            <field name="interval_type">hours</field>
            <field name="active">True</field>
            <field name="priority">5</field>
            <field name="user_id" ref="base.user_root"/>
        </record>
        

        <record id="cron_auto_archive_documents" model="ir.cron">
            <field name="name">Auto Archive Expired Documents</field>
            <field name="model_id" ref="model_document_document"/>
            <field name="state">code</field>
            <field name="code">model.auto_archive_expired_documents()</field>
            <field name="interval_number">1</field>
            <field name="interval_type">days</field>
            <field name="active">True</field>
            <field name="priority">3</field>
            <field name="user_id" ref="base.user_root"/>
        </record>
        

        <record id="cron_send_reminders" model="ir.cron">
            <field name="name">Send Approval Reminders</field>
            <field name="model_id" ref="model_approval_request"/>
            <field name="state">code</field>
            <field name="code">model._cron_send_approval_reminders()</field>
            <field name="interval_number">2</field>
            <field name="interval_type">hours</field>
            <field name="active">True</field>
            <field name="priority">4</field>
            <field name="user_id" ref="base.user_root"/>
        </record>

        <record id="cron_auto_route_new_documents" model="ir.cron">
            <field name="name">Auto Route New Documents</field>
            <field name="model_id" ref="model_document_router"/>
            <field name="state">code</field>
            <field name="code">model.auto_route_new_documents()</field>
            <field name="interval_number">1</field>
            <field name="interval_type">hours</field>
            <field name="active">True</field>
            <field name="priority">6</field>
            <field name="user_id" ref="base.user_root"/>
        </record>
        

        <record id="cron_update_performance_metrics" model="ir.cron">
            <field name="name">Update Performance Metrics</field>
            <field name="model_id" ref="model_document_document"/>
            <field name="state">code</field>
            <field name="code">model.update_performance_metrics()</field>
            <field name="interval_number">6</field>
            <field name="interval_type">hours</field>
            <field name="active">True</field>
            <field name="priority">2</field>
            <field name="user_id" ref="base.user_root"/>
        </record>
        

        <record id="cron_cleanup_temp_data" model="ir.cron">
            <field name="name">Auto Cleanup Temporary Data</field>
            <field name="model_id" ref="model_document_document"/>
            <field name="state">code</field>
            <field name="code">model.cleanup_temporary_data()</field>
            <field name="interval_number">1</field>
            <field name="interval_type">weeks</field>
            <field name="active">True</field>
            <field name="priority">1</field>
            <field name="user_id" ref="base.user_root"/>
        </record>
        

        
        <record id="cron_escalate_overdue_workflow_instances" model="ir.cron">
            <field name="name">Auto Escalate Overdue Workflow Instances</field>
            <field name="model_id" ref="model_document_workflow_instance"/>
            <field name="state">code</field>
            <field name="code">model.check_overdue_instances()</field>
            <field name="interval_number">2</field>
            <field name="interval_type">hours</field>
            <field name="active">True</field>
            <field name="priority">7</field>
            <field name="user_id" ref="base.user_root"/>
        </record>
        
        
        <record id="cron_cleanup_test_instances" model="ir.cron">
            <field name="name">Auto Cleanup Test Instances</field>
            <field name="model_id" ref="model_document_workflow_instance"/>
            <field name="state">code</field>
            <field name="code">model.cleanup_test_instances()</field>
            <field name="interval_number">1</field>
            <field name="interval_type">weeks</field>
            <field name="active">True</field>
            <field name="priority">1</field>
            <field name="user_id" ref="base.user_root"/>
        </record>
        
        
        <record id="cron_update_process_statistics" model="ir.cron">
            <field name="name">Update Process Statistics</field>
            <field name="model_id" ref="model_workflow_process"/>
            <field name="state">code</field>
            <field name="code"><![CDATA[
active_processes = model.search([('state', '=', 'active')])
log('Updated statistics for %s processes' % len(active_processes))
            ]]></field>
            <field name="interval_number">6</field>
            <field name="interval_type">hours</field>
            <field name="active">True</field>
            <field name="priority">3</field>
            <field name="user_id" ref="base.user_root"/>
        </record>
        
        
        <record id="cron_send_task_due_reminders" model="ir.cron">
            <field name="name">Send Task Due Reminders</field>
            <field name="model_id" ref="model_admin_task"/>
            <field name="state">code</field>
            <field name="code"><![CDATA[

model.send_due_task_reminders()
            ]]></field>
            <field name="interval_number">12</field>
            <field name="interval_type">hours</field>
            <field name="active">True</field>
            <field name="priority">5</field>
            <field name="user_id" ref="base.user_root"/>
        </record>
        
        
        <record id="cron_auto_trigger_ready_processes" model="ir.cron">
            <field name="name">Auto Trigger Ready Processes</field>
            <field name="model_id" ref="model_document_document"/>
            <field name="state">code</field>
            <field name="code"><![CDATA[
ready_documents = model.search([('processing_state', '=', 'registered'), ('workflow_id', '=', False)])
log('Processed %s documents' % len(ready_documents))
            ]]></field>
            <field name="interval_number">1</field>
            <field name="interval_type">hours</field>
            <field name="active">True</field>
            <field name="priority">6</field>
            <field name="user_id" ref="base.user_root"/>
        </record>
        
        
        <record id="cron_query_optimization" model="ir.cron">
            <field name="name">Query Optimization</field>
            <field name="model_id" ref="model_document_document"/>
            <field name="state">code</field>
            <field name="code"><![CDATA[
documents_with_tasks = model.search([('task_ids', '!=', False)], limit=50)
log('Updated %s documents' % len(documents_with_tasks))
            ]]></field>
            <field name="interval_number">6</field>
            <field name="interval_type">hours</field>
            <field name="active">True</field>
            <field name="priority">3</field>
            <field name="user_id" ref="base.user_root"/>
        </record>
        

        <record id="cron_transaction_health_check" model="ir.cron">
            <field name="name">Transaction Health Check</field>
            <field name="model_id" ref="model_admin_task"/>
            <field name="state">code</field>
            <field name="code"><![CDATA[
completed_tasks = model.search([('state', '=', 'completed'), ('progress_percentage', '<', 100)])
completed_tasks.write({'progress_percentage': 100.0})
progress_tasks = model.search([('state', '=', 'in_progress'), ('progress_percentage', '=', 0)])
progress_tasks.write({'progress_percentage': 10.0})
log('Fixed %s tasks' % (len(completed_tasks) + len(progress_tasks)))
            ]]></field>
            <field name="interval_number">4</field>
            <field name="interval_type">hours</field>
            <field name="active">True</field>
            <field name="priority">4</field>
            <field name="user_id" ref="base.user_root"/>
        </record>
        

        <record id="cron_sla_optimization" model="ir.cron">
            <field name="name">SLA Optimization</field>
            <field name="model_id" ref="model_document_category"/>
            <field name="state">code</field>
            <field name="code"><![CDATA[
categories = model.search([])
log('Updated %s categories' % len(categories))
            ]]></field>
            <field name="interval_number">1</field>
            <field name="interval_type">days</field>
            <field name="active">True</field>
            <field name="priority">2</field>
            <field name="user_id" ref="base.user_root"/>
        </record>
        
    </data>
</odoo>
